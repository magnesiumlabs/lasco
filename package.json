{
    "name": "lasco",
    "version": "0.11.0",
    "description": "The Sass utilities classes library.",
    "scripts": {
        "clean:chunks": "rimraf \"dist/*.js*\"",
        "clean:dist": "rimraf \"dist/*\"",
        "clean": "npm-run-all --parallel clean:*",
        "build": "npm run clean:dist && webpack && npm run clean:chunks",
        "prepare": "husky install && npm run build",
        "release:patch": "npm run release -- --release-as patch",
        "release:minor": "npm run release -- --release-as minor",
        "release:major": "npm run release -- --release-as major",
        "release:dry": "npm run release -- --dry-run",
        "release": "standard-version"
    },
    "engines": {
        "node": ">=12.0.0",
        "npm": ">=7.0.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^16.0.1",
        "@commitlint/config-conventional": "^16.0.0",
        "autoprefixer": "^10.4.1",
        "css-loader": "^6.5.1",
        "css-minimizer-webpack-plugin": "^3.3.1",
        "husky": "^7.0.4",
        "mini-css-extract-plugin": "^2.4.5",
        "npm-run-all": "^4.1.5",
        "postcss-loader": "^6.2.1",
        "rimraf": "^3.0.2",
        "sass-loader": "^12.4.0",
        "standard-version": "^9.3.2",
        "webpack": "^5.65.0",
        "webpack-cli": "^4.9.1"
    },
    "dependencies": {
        "@sass-collective/breakpoint": "^2.4.1",
        "@sass-collective/css": "^2.0.2",
        "sass": "^1.45.2"
    },
    "files": [
        "*.scss",
        "src/**/*"
    ],
    "keywords": [
        "lasco",
        "sass",
        "utilities",
        "library",
        "css",
        "front-end"
    ],
    "license": "MIT",
    "homepage": "https://github.com/magnesiumlabs/lasco/tree/main",
    "repository": {
        "type": "git",
        "url": "https://github.com/magnesiumlabs/lasco.git"
    },
    "bugs": {
        "url": "https://github.com/magnesiumlabs/lasco/issues"
    }
}
