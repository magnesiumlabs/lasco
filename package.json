{
    "name": "lasco",
    "version": "2.5.0",
    "description": "The Sass utilities classes library.",
    "scripts": {
        "dev:docs": "vitepress dev docs",
        "clean:chunks": "rimraf \"dist/*.js*\"",
        "clean:dist": "rimraf \"dist/*\"",
        "clean": "npm-run-all --parallel clean:*",
        "test": "jest --ci",
        "build": "npm run clean:dist && webpack && npm run clean:chunks",
        "build:docs": "vitepress build docs",
        "prepare": "husky install && npm run build",
        "release": "semantic-release"
    },
    "engines": {
        "node": ">=16.0.0",
        "npm": ">=8.0.0"
    },
    "sass": "index.scss",
    "devDependencies": {
        "@commitlint/cli": "^17.4.2",
        "@commitlint/config-conventional": "^17.4.2",
        "@semantic-release/changelog": "^6.0.2",
        "@semantic-release/git": "^10.0.1",
        "autoprefixer": "^10.4.13",
        "css-loader": "^6.7.1",
        "css-minimizer-webpack-plugin": "^4.0.0",
        "glob": "^7.2.3",
        "husky": "^8.0.3",
        "jest": "^29.3.1",
        "jest-environment-node-single-context": "^29.0.0",
        "jest-junit": "^15.0.0",
        "mini-css-extract-plugin": "^2.6.1",
        "npm-run-all": "^4.1.5",
        "postcss-loader": "^7.0.1",
        "postcss-sort-media-queries": "^4.3.0",
        "rimraf": "^3.0.2",
        "sass-loader": "^13.0.2",
        "sass-true": "^7.0.0",
        "semantic-release": "^20.1.1",
        "vitepress": "^1.0.0-alpha.49",
        "vue": "^3.2.45",
        "webpack": "^5.76.2",
        "webpack-cli": "^5.0.1"
    },
    "dependencies": {
        "@unsass/breakpoint": "^2.1.2",
        "@unsass/css": "^2.1.0",
        "@unsass/selector": "^1.2.0",
        "@unsass/string": "^1.0.0",
        "sass": "^1.59.3"
    },
    "files": [
        "*.scss",
        "src/**/*",
        "CHANGELOG.md"
    ],
    "keywords": [
        "sass",
        "utilities",
        "library",
        "lasco",
        "front-end"
    ],
    "license": "MIT",
    "homepage": "https://github.com/magnesiumlabs/lasco/tree/main",
    "repository": {
        "type": "git",
        "url": "https://github.com/magnesiumlabs/lasco.git"
    },
    "bugs": {
        "url": "https://github.com/magnesiumlabs/lasco/issues"
    }
}
