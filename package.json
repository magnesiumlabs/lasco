{
    "name": "lasco",
    "version": "0.16.0",
    "description": "The Sass utilities classes library.",
    "scripts": {
        "clean:chunks": "rimraf \"dist/*.js*\"",
        "clean:dist": "rimraf \"dist/*\"",
        "clean": "npm-run-all --parallel clean:*",
        "build": "npm run clean:dist && webpack && npm run clean:chunks",
        "prepare": "husky install && npm run build",
        "release": "semantic-release --branches main"
    },
    "engines": {
        "node": ">=14.0.0",
        "npm": ">=8.0.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^16.1.0",
        "@commitlint/config-conventional": "^16.0.0",
        "@semantic-release/changelog": "^6.0.1",
        "@semantic-release/git": "^10.0.1",
        "autoprefixer": "^10.4.2",
        "css-loader": "^6.5.1",
        "css-minimizer-webpack-plugin": "^3.4.1",
        "husky": "^7.0.4",
        "mini-css-extract-plugin": "^2.5.3",
        "npm-run-all": "^4.1.5",
        "postcss-loader": "^6.2.1",
        "rimraf": "^3.0.2",
        "sass-loader": "^12.4.0",
        "semantic-release": "^19.0.2",
        "webpack": "^5.67.0",
        "webpack-cli": "^4.9.2"
    },
    "dependencies": {
        "@sass-collective/breakpoint": "^2.4.1",
        "@sass-collective/css": "^2.0.2",
        "@sass-collective/property": "^4.6.3",
        "sass": "^1.49.0"
    },
    "files": [
        "*.scss",
        "src/**/*"
    ],
    "keywords": [
        "lasco",
        "sass",
        "utilities",
        "library",
        "css",
        "front-end"
    ],
    "license": "MIT",
    "homepage": "https://github.com/magnesiumlabs/lasco/tree/main",
    "repository": {
        "type": "git",
        "url": "https://github.com/magnesiumlabs/lasco.git"
    },
    "bugs": {
        "url": "https://github.com/magnesiumlabs/lasco/issues"
    }
}
